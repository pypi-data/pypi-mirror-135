{
  "env": {
    "DCOS_PATH_SERVICE_NODE": "/hsds/hsdssn",
    "AWS_S3_GATEWAY": "http://192.168.99.99:9999",
    "NODE_TYPE": "sn",
    "HEAD_ENDPOINT": "http://hsdshsdshead.marathon.l4lb.thisdcos.directory:5100",
    "AWS_ACCESS_KEY_ID": "your_aws_key",
    "AWS_SECRET_ACCESS_KEY": "your_aws_secret",
    "DCOS_PATH_DATA_NODE": "/hsds/hsdsdn",
    "PASSWORD_FILE": "/pathto/passwd.txt",
    "BUCKET_NAME": "your_bucketname"
  },
  "id": "/hsds/hsdssn",
  "backoffFactor": 1.15,
  "backoffSeconds": 1,
  "container": {
    "portMappings": [
      {
        "containerPort": 5101,
        "hostPort": 0,
        "labels": {
          "VIP_0": "/hsds/hsdssn:8080"
        },
        "protocol": "tcp",
        "name": "sn"
      }
    ],
    "type": "DOCKER",
    "volumes": [],
    "docker": {
      "image": "<your_repo:port>/hdfgroup/hsds",
      "forcePullImage": true,
      "privileged": false,
      "parameters": []
    }
  },
  "cpus": 0.25,
  "disk": 0,
  "instances": 1,
  "maxLaunchDelaySeconds": 300,
  "mem": 1024,
  "gpus": 0,
  "networks": [
    {
      "mode": "container/bridge"
    }
  ],
  "requirePorts": false,
  "upgradeStrategy": {
    "maximumOverCapacity": 1,
    "minimumHealthCapacity": 1
  },
  "killSelection": "YOUNGEST_FIRST",
  "unreachableStrategy": {
    "inactiveAfterSeconds": 0,
    "expungeAfterSeconds": 0
  },
  "healthChecks": [],
  "constraints": []
}
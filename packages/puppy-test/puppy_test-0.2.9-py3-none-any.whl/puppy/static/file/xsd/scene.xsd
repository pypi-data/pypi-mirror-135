<?xml version="1.0"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:vc="http://www.w3.org/2007/XMLSchema-versioning"
           vc:minVersion="1.1"
           targetNamespace="http://www.w3school.com.cn"
           xmlns="http://www.w3school.com.cn"
           elementFormDefault="qualified">
    <xs:element name="scene" type="scene">
        <xs:unique name="id不可重复">
            <xs:selector xpath=".//*"/>
            <xs:field xpath="@id"/>
        </xs:unique>
    </xs:element>
    <xs:complexType name="scene">
        <xs:annotation>
            <xs:documentation><![CDATA[
            场景
            中文名：场景
            类型：标签
            ]]></xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="db_info" type="db_info_for_label" minOccurs="0"/>
            <xs:element name="case" type="case" maxOccurs="unbounded"/>
        </xs:sequence>
        <xs:attribute name="author_work_number">
            <xs:annotation>
                <xs:documentation><![CDATA[
                   作者工号
                ]]></xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="author_name">
            <xs:annotation>
                <xs:documentation><![CDATA[
                   作者姓名
                ]]></xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType name="case">
        <xs:annotation>
            <xs:documentation><![CDATA[
                案例
                中文名：案例
                类型：标签
            ]]></xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:group ref="common" maxOccurs="unbounded"/>
        </xs:sequence>
        <xs:attribute name="id" use="required">
            <xs:annotation>
                <xs:documentation><![CDATA[
                    案例的编号
                    中文名：编号
                    类型：属性
                    格式：
                        --不能为空
                        --只能是数字
                ]]></xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:minLength value="1"/>
                    <xs:pattern value="[0-9]*"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="name" use="required">
            <xs:annotation>
                <xs:documentation><![CDATA[
                    案例的名称
                    中文名：名称
                    类型：属性
                    格式：
                    --不能为空
                    --只能包含字母数字中文
                    --只能包含中文/英文逗号、中文句号、中文冒号、中文分号、中文/英文括号、中文/英文感叹号、中文分号、加号、减号及下划线
                    --长度不能超过100个字符
                ]]></xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:pattern value="[\w_，。；：（）！、,()!+-]{1,100}"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="db_info" type="db_info"/>
        <xs:attribute name="author_work_number"/>
        <xs:attribute name="author_name"/>
    </xs:complexType>
    <xs:complexType name="interface">
        <xs:annotation>
            <xs:documentation><![CDATA[
                接口
                中文名：接口
                类型：标签
            ]]></xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="pre_processor" type="pre_processor" minOccurs="0"/>
            <xs:element name="post_processor" type="postProcessorType" minOccurs="0"/>
        </xs:sequence>
        <xs:attribute name="name" use="required">
            <xs:annotation>
                <xs:documentation><![CDATA[
                    被调用接口的名称
                    中文名：名称
                    类型：属性
                    格式：
                    --不能为空
                    --来自"interface"文件夹下接口定义XML的文件名
                ]]></xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:minLength value="1"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="db_info" type="db_info"/>
        <xs:attribute name="desc" use="required">
            <xs:annotation>
                <xs:documentation><![CDATA[
                    被调用接口的作用描述
                    中文名：描述
                    类型：属性
                ]]></xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="session" default="inherit">
            <xs:annotation>
                <xs:documentation><![CDATA[
                    使用的session
                    中文名：描述
                    类型：属性
                    说明：默认基础上一个接口的，此属性仅对http接口有效
                ]]></xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction>
                    <xs:enumeration value="inherit"/>
                    <xs:enumeration value="new"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="sequenceId"/>
    </xs:complexType>
    <xs:complexType name="selenium">
        <xs:annotation>
            <xs:documentation><![CDATA[
                UI函数调用
                中文名：网页UI函数调用
                类型：标签
            ]]></xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="param" minOccurs="0">
                <xs:annotation>
                    <xs:documentation><![CDATA[
                        用来设置UI操作函数的实参
                        中文名：设置实参
                        类型：标签
                    ]]></xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:simpleContent>
                        <xs:extension base="xs:string">
                            <xs:attribute name="name" use="required">
                                <xs:annotation>
                                    <xs:documentation><![CDATA[
                                实参的名称
                                中文名：名称
                                类型：属性
                            ]]></xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:minLength value="1"/>
                                        <xs:pattern value="[a-zA-Z_][a-zA-Z0-9_]*"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:attribute>
                        </xs:extension>
                    </xs:simpleContent>
                </xs:complexType>
            </xs:element>
            <xs:element name="set" minOccurs="0">
                <xs:annotation>
                    <xs:documentation><![CDATA[
                        用来将UI操作函数返回的结果设置到局部变量池
                        中文名：设置变量
                        类型：标签
                    ]]></xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:simpleContent>
                        <xs:extension base="xs:string">
                            <xs:attribute name="name" type="variable_name" use="required"/>
                            <xs:attribute name="desc"/>
                        </xs:extension>
                    </xs:simpleContent>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="module" use="required">
            <xs:annotation>
                <xs:documentation><![CDATA[
                    将调用函数的模块
                    中文名：模块名称
                    类型：属性
                    格式：
                    --不能为空
                    --必须是来自项目根目录下"seleniums"文件夹下的文件名，无需包含后缀"py"
                ]]></xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:minLength value="1"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="func" use="required">
            <xs:annotation>
                <xs:documentation><![CDATA[
                    将调用的函数的名称
                    中文名：函数名称
                    类型：属性
                    格式：
                    --不能为空
                    --具体module内的函数名称
                ]]></xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:minLength value="1"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="desc" use="required">
            <xs:annotation>
                <xs:documentation><![CDATA[
                    被调用ui操作的描述
                    中文名：描述
                    类型：属性
                ]]></xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="driver">
            <xs:annotation>
                <xs:documentation><![CDATA[
                    自动化UI执行驱动，需要和对应的浏览器对应
                    中文名：selenium的驱动
                    类型：属性
                    其他：
                    --为了能在服务器上执行脚本，建议不用设置此属性
                    --自行指定的驱动必须是全路径
                ]]></xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="block_cookie">
            <xs:annotation>
                <xs:documentation><![CDATA[
                    阻止cookie，默认为不阻止
                    中文名：阻止cookie
                    类型：属性
                ]]></xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="browser">
            <xs:annotation>
                <xs:documentation><![CDATA[
                    使用的浏览器，默认Chrome
                    中文名：浏览器
                    类型：属性
                ]]></xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:enumeration value="Chrome">
                        <xs:annotation>
                            <xs:documentation><![CDATA[
                                                中文名：谷歌浏览器
                                                类型：属性值
                                            ]]></xs:documentation>
                        </xs:annotation>
                    </xs:enumeration>
                    <xs:enumeration value="Firefox">
                        <xs:annotation>
                            <xs:documentation><![CDATA[
                                                中文名：火狐浏览器
                                                类型：属性值
                                            ]]></xs:documentation>
                        </xs:annotation>
                    </xs:enumeration>
                    <xs:enumeration value="Edge">
                        <xs:annotation>
                            <xs:documentation><![CDATA[
                                                中文名：微软Edge浏览器
                                                类型：属性值
                                            ]]></xs:documentation>
                        </xs:annotation>
                    </xs:enumeration>
                    <xs:enumeration value="none">
                        <xs:annotation>
                            <xs:documentation><![CDATA[
                                                中文名：微软IE浏览器
                                                类型：属性值
                                            ]]></xs:documentation>
                        </xs:annotation>
                    </xs:enumeration>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType name="pre_processor">
        <xs:annotation>
            <xs:documentation><![CDATA[
                前置处理器，用来执行接口调用前数据构造、预期条件准备、接口各项数据的替换，插入，删除等工作
                中文名：前置处理器
                类型：标签
            ]]></xs:documentation>
        </xs:annotation>
        <xs:group ref="pre"/>
        <xs:attribute name="db_info" type="db_info"/>
    </xs:complexType>
    <xs:complexType name="postProcessorType">
        <xs:annotation>
            <xs:documentation><![CDATA[
                后置处理器，用来执行接口调用后的数据清理、数据获取、断言等工作
                中文名：后置处理器
                类型：标签
            ]]></xs:documentation>
        </xs:annotation>
        <xs:group ref="post"/>
        <xs:attribute name="db_info" type="db_info"/>
    </xs:complexType>
    <xs:complexType name="ifPostType">
        <xs:group ref="post"/>
        <xs:attribute name="ex" use="required" type="ex"/>
        <xs:attribute name="desc" use="required" type="descOfIfLabel"/>
    </xs:complexType>
    <xs:complexType name="ifPostType_inner">
        <xs:sequence>
            <xs:group ref="post" maxOccurs="unbounded" minOccurs="0"/>
            <xs:choice minOccurs="0">
                <xs:element name="break" type="break"/>
                <xs:element name="continue" type="continue"/>
            </xs:choice>
        </xs:sequence>
        <xs:attribute name="ex" use="required" type="ex"/>
        <xs:attribute name="desc" use="required" type="descOfIfLabel"/>
    </xs:complexType>
    <xs:complexType name="ifPreType">
        <xs:group ref="pre"/>
        <xs:attribute name="ex" use="required" type="ex"/>
        <xs:attribute name="desc" use="required" type="descOfIfLabel"/>
    </xs:complexType>
    <xs:complexType name="ifPreType_inner">
        <xs:sequence>
            <xs:group ref="pre" maxOccurs="unbounded" minOccurs="0"/>
            <xs:choice minOccurs="0">
                <xs:element name="break" type="break"/>
                <xs:element name="continue" type="continue"/>
            </xs:choice>
        </xs:sequence>
        <xs:attribute name="ex" use="required" type="ex"/>
        <xs:attribute name="desc" use="required" type="descOfIfLabel"/>
    </xs:complexType>
    <xs:complexType name="if">
        <xs:sequence>
            <xs:group ref="common" maxOccurs="unbounded" minOccurs="0"/>
        </xs:sequence>
        <xs:attribute name="ex" use="required" type="ex"/>
        <xs:attribute name="desc" use="required" type="descOfIfLabel"/>
    </xs:complexType>
    <xs:complexType name="if_inner">
        <xs:sequence>
            <xs:group ref="common" maxOccurs="unbounded" minOccurs="0"/>
            <xs:choice minOccurs="0">
                <xs:element name="break" type="break"/>
                <xs:element name="continue" type="continue"/>
            </xs:choice>
        </xs:sequence>
        <xs:attribute name="ex" use="required" type="ex"/>
        <xs:attribute name="desc" use="required" type="descOfIfLabel"/>
    </xs:complexType>
    <xs:complexType name="while">
        <xs:group ref="cycle"/>
        <xs:attribute name="ex" use="required" type="ex"/>
        <xs:attribute name="desc" use="required"/>
        <xs:attribute name="count"/>
    </xs:complexType>
    <xs:complexType name="for">
        <xs:annotation>
            <xs:documentation><![CDATA[
                对列表或字典进行遍历
                中文名：循环遍历
                类型：标签
            ]]></xs:documentation>
        </xs:annotation>
        <xs:group ref="cycle"/>
        <xs:attribute name="key" use="required">
            <xs:annotation>
                <xs:documentation><![CDATA[
                    单项名称
                    中文名：单项名称
                    类型：属性
                    格式：
                        --如果遍历的是列表，仅需提供1个名称
                        --如果遍历的是字典，需提供2个名称，用英文逗号隔开
                ]]></xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="items" use="required">
            <xs:annotation>
                <xs:documentation><![CDATA[
                    待遍历的数据来源，可以是列表或字典
                    中文名：数据来源
                    类型：属性
                ]]></xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="desc" use="required"/>
    </xs:complexType>
    <xs:simpleType name="id">
        <xs:annotation>
            <xs:documentation>
                案例的id，只能是数字。
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:pattern value="[0-9]*"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="不能为空">
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="descOfIfLabel">
        <xs:annotation>
            <xs:documentation><![CDATA[
                    分支标签的描述
                    中文名：描述
                    类型：属性
                ]]></xs:documentation>
        </xs:annotation>
    </xs:complexType>
    <xs:complexType name="break">
        <xs:annotation>
            <xs:documentation>
                终止当前循环
                中文名：终止
                类型：属性
            </xs:documentation>
        </xs:annotation>
    </xs:complexType>
    <xs:complexType name="continue">
        <xs:annotation>
            <xs:documentation>
                终止当次循环
                中文名：继续
                类型：属性
            </xs:documentation>
        </xs:annotation>
    </xs:complexType>
    <xs:complexType name="set">
        <xs:annotation>
            <xs:documentation><![CDATA[
                设置变量到局部变量池,此标签一般用作案例执行前的某些公共参数的赋值
                中文名：设置变量
                类型：标签
            ]]></xs:documentation>
        </xs:annotation>
        <xs:simpleContent>
            <xs:extension base="xs:string">
                <xs:attribute name="name" use="required" type="variable_name"/>
                <xs:attribute name="value">
                    <xs:annotation>
                        <xs:documentation><![CDATA[
                        变量值
                        中文名：变量值
                        类型：属性
                        格式：
                            --不可为空
                            --字符串或${}表达式
                     ]]></xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:minLength value="1"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:attribute>
                <xs:attribute name="var_type" type="var_type"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
    <xs:simpleType name="var_type">
        <xs:annotation>
            <xs:documentation><![CDATA[
                变量的类型
                中文名：变量的类型
                类型：属性
                举例：
                    --value为纯${}表达式时，不设置此属性，变量的类型为表达式实际计算结果的类型
                    --value不含${}表达式或混合体时，不设置此属性，变量的类型为str
                    --无论何种情况，只要设置了此属性，变量将尝试转型，若转型失败将抛出异常
            ]]></xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="int">
                <xs:annotation>
                    <xs:documentation>整型</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="float">
                <xs:annotation>
                    <xs:documentation>浮点数/小数</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="bool">
                <xs:annotation>
                    <xs:documentation>布尔值</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="str">
                <xs:annotation>
                    <xs:documentation>字符串</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="auto">
                <xs:annotation>
                    <xs:appinfo>自动推断</xs:appinfo>
                    <xs:documentation>自动推测为整形、浮点数、布尔值、字符串</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="db_info">
        <xs:annotation>
            <xs:documentation><![CDATA[
                    用来指明该标签下的数据库连接配置信息
                    中文名：数据库连接信息
                    类型：属性
                    格式：/mysql|oracle/用户名/密码/地址[:端口]/服务名或数据库名
                ]]></xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="/(mysql|oracle)/([^/\s]*)/([^/\s]*)/([^/\s:]*):?([0-9]*)?/([^/\s]*)"/>
            <xs:pattern value=".*"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="variable_name">
        <xs:annotation>
            <xs:documentation><![CDATA[
                                变量的名称
                                中文名：名称
                                类型：属性
                                格式：
                                --不能为空
                                --只能包含字母、数字、下划线且不能以数字开头
                            ]]></xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:pattern value="[a-zA-Z_][a-zA-Z0-9_,]*"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="db_info_for_label">
        <xs:annotation>
            <xs:documentation><![CDATA[
                    用来说明该场景中的数据库连接配置信息
                    中文名：数据库连接信息
                    类型：标签
                    格式：/mysql|oracle/用户名/密码/地址[:端口]/服务名或数据库名
                ]]></xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="/(mysql|oracle)/([^/\s]*)/([^/\s]*)/([^/\s:]*):?([0-9]*)?/([^/\s]*)"/>
            <xs:pattern value=".*"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="flow">
        <xs:annotation>
            <xs:documentation><![CDATA[
                            指明被调用的流程，被调用的流程定义在项目根目录的"flow"文件夹下
                            中文名：流程
                            类型：标签
                        ]]></xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="set" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation><![CDATA[
                        中文名：设置变量
                        类型：标签
                        说明：
                            --用来将流程返回的结果设置到局部变量池
                            --流程返回多个结果时，按顺序赋值给对应的set标签
                    ]]></xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:attribute name="name" use="required" type="variable_name"/>
                    <xs:attribute name="desc"/>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="name" use="required">
            <xs:annotation>
                <xs:documentation><![CDATA[
                    中文名：名称
                    类型：属性
                    说明：被调用流程的名称
                    格式：
                    --不能为空
                    --来自"flow"文件夹下流程定义XML的文件名
                ]]></xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:minLength value="1"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:anyAttribute processContents="skip"/>
    </xs:complexType>
    <xs:simpleType name="key">
        <xs:annotation>
            <xs:documentation><![CDATA[
                指明提取数据来源中的哪一部分的数据，不指定时默认为$whole
                中文名：提取键
                类型：属性
                格式：点缀提取键、数字切片、正则提取键、$whole、$rows、$columns
                举例：
                    --提取json数据“{"a":{"b":1}}”中的1，使用点缀提取键：a.b
                    --提取json数据“{"a":[1,2,3]}”中的2，使用点缀提取键：a[1]
                    --提取数据库查询结果第1行1列，使用数字切片：0,0
                    --提取数据库操作行数或查询结果行数，使用$rows
                    --提取字符串“puppy”中的第3个字母p，使用正则提取键：$regex[2]:p
                    --提取数据来源中的所有数据，使用$whole
    ]]></xs:documentation>
        </xs:annotation>
        <xs:union>
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:pattern value=".+"/>
                </xs:restriction>
            </xs:simpleType>
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:enumeration value="$whole"/>
                    <xs:enumeration value="$rows"/>
                    <xs:enumeration value="$columns"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:union>

    </xs:simpleType>
    <xs:simpleType name="sql">
        <xs:annotation>
            <xs:documentation><![CDATA[
                    sql语句
                    中文名：sql语句
